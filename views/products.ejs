<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/product.css">
</head>
<body>
    <div id="main">
        <div id="nav">
            <div id="logo">logo</div>
            <div id="right">
                <a href="#">Home</a><a href="#">Cart</a><a href="#">Bot</a><a href="#">Logout</a>
            </div>
        </div>
        <div id="page1">
            <div id="search">
                <input type="search" name="filter" id="filter" placeholder="Search items here">
            </div>
            <div id="products">
    <% products.forEach((e)=>{ %>
                <div class="product">
                    <div class="image"></div>
                    <div class="data">
                        <h3>e.title</h3>
                        <h3>e.price</h3>
                        <h3>e.brand</h3>
                        <a href="/product/<%= e._id %>">see more</a>
                        <a href="">addtocart</a>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>

<script src="/javascripts/product.js"></script>   
</body>
</html>